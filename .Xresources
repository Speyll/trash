! font settings
*.font: Ioveska Term:size=12
urxvt.font: xft: Ioveska Term:size=12
dmenu.font: Ioveska Term:size=12

Xft.hinting: true
Xft.autohint: true
Xft.rgba: true
Xft.lcdfilter: lcddefault

! urxvt settings
urxvt.termName: rxvt-unicode-256color
urxvt.scrollBar: false
urxvt.scrollbar_right: false
urxvt.internalBorder: 25
urxvt.cursorBlink: true
! urxvt.transparent: true
! urxvt.shading: 10

! sxiv settings
Sxiv.font: Ioveska Term
Sxiv.foreground: #f2f2f2
Sxiv.background: #0c0c0c

! general
*.alpha: 0.9
*background: rgb:c/c/c
*foreground: rgb:ee/e8/d5

! blacks
*color0: rgb:58/6e/75
*color8: rgb:65/7b/83

! reds
